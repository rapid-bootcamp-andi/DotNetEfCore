@model DotnetEfCore.DataContext.TokoKomputerEntity

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<div class="container">
    <h1></h1>
</div><!-- End Breadcrumbs -->

<section id="contact" class="contact section-bg">
    <div class="container" data-aos="fade-up">
        &nbsp;
        <div class="section-header">
            <h2>Detail Produk</h2>
        </div>
        <div class="container section-bg" style="overflow: auto;">
            <p></p>
            <table class="table table-bordered" style="width:322px">
                <tr>
                    <th>Id</th>
                    <td>@Model.Id</td>
                </tr>
                <tr>
                    <th>Nama Produk</th>
                    <td>@Model.Nama</td>
                </tr>
                <tr>
                    <th>Kategori</th>
                    <td>@Model.Kategori</td>
                </tr>
                <tr>
                    <th>Jumlah</th>
                    <td>@Model.Jumlah</td>
                </tr>
                <tr>
                    <th>Harga</th>
                    <td>@Model.Harga</td>
                </tr>
            </table>
        </div>

        <!--End Contact Form -->

    </div>
</section>
